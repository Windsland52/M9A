    "EventLaplaceBoxesClaim": {
        "doc": "领取观测者之盒的次数",
        "recognition": {
            "type": "OCR",
            "param": {
                "expected": "领取",
                "roi": [
                    1046,
                    397,
                    88,
                    195
                ]
            }
        },
        "action": {
            "type": "Click"
        },
