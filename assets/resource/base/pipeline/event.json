            "BackButton"
        ]
    },
    "EventLaplaceFoxesNumber": {
        "doc": "判断可观测次数",
        "recognition": {
            "type": "OCR",
            "param": {
                "expected": "^[1-9][0-9]?$",
                "only_rec": true,
                "roi": [
                    897,
                    282,
                    47,
                    61
                ]
            }
        },
        "action": "DoNothing",
        "next": [
            "EventLaplaceFoxesClaim", // 若有可观测次数则尝试用次数来观测盒子  
            "[JumpBack]ObtainedAwards", //结算领取页面
            "BackButton"
